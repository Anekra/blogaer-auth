{
  "name": "encore-ts-starter",
  "private": true,
  "version": "0.0.1",
  "description": "Encore Typescript Starter",
  "license": "MPL-2.0",
  "type": "module",
  "scripts": {
    "dev": ". .env && encore run --port 3030",
    "build": "encore build docker auth-service:latest",
    "lint": "biome check . --apply",
    "db:drop": ". .env && bun x sequelize-cli db:drop",
    "db:create": ". .env && bun x sequelize-cli db:create",
    "db:migrate": ". .env && bun x sequelize-cli db:migrate",
    "db:migrate:undo": ". .env && bun x sequelize-cli db:migrate:undo",
    "db:migrate:undo:all": ". .env && bun x sequelize-cli db:migrate:undo:all",
    "db:seed:all": ". .env && bun x sequelize-cli db:seed:all",
    "db:seed:role": ". .env && bun x sequelize-cli db:seed --seed ./src/sequelize/seeders/default-roles.ts",
    "db:seed:undo:all": ". .env && bun x sequelize-cli db:seed:undo:all"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.5",
    "@types/amqplib": "^0.10.7",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.5.7",
    "@types/nodemailer": "^6.4.17",
    "@types/qrcode": "^1.5.5",
    "sequelize-cli": "sequelize/cli",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@sequelize/sqlite3": "^7.0.0-alpha.46",
    "@simplewebauthn/server": "^13.1.1",
    "amqplib": "^0.10.8",
    "bcryptjs": "^3.0.2",
    "encore.dev": "1.52.3",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^7.0.4",
    "otplib": "^12.0.1",
    "qrcode": "^1.5.4",
    "sequelize": "^6.37.7",
    "ua-parser-js": "^2.0.3"
  }
}